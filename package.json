{
  "name": "vue-concurrency",
  "version": "4.0.0-8",
  "description": "A library for encapsulating asynchronous operations and managing concurrency for Vue + Composition API",
  "source": "src/index.ts",
  "module": "dist/index.module.js",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "scripts"
  ],
  "scripts": {
    "build:vue3": "microbundle build -o dist/vue3 --tsconfig tsconfig.build.json --external vue3,vue,caf --alias ./api=./api3,./utils/api=./utils/api3 && node ./scripts/fixVue3Import.js",
    "build:vue2": "microbundle build -o dist/vue2 --tsconfig tsconfig.build.json --external caf,vue2,vue,@vue/composition-api",
    "build": "npm run build:vue2",
    "dev": "microbundle watch",
    "test": "VUE=2 jest && VUE=3 jest",
    "test:vue2": "jest",
    "test:vue3": "VUE=3 jest",
    "prepublishOnly": "npm run build",
    "postinstall": "node ./scripts/postinstall.js",
    "test:coverage": "jest --coverage",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "release-it"
  },
  "keywords": [
    "vue",
    "composition api",
    "vuejs",
    "generators",
    "concurrency",
    "task",
    "async",
    "hooks"
  ],
  "author": "Martin Malinda",
  "repository": "github:martinmalinda/vue-concurrency",
  "homepage": "https://vue-concurrency.netlify.app/",
  "license": "MIT",
  "dependencies": {
    "caf": "^15.0.0-preB"
  },
  "peerDependencies": {
    "vue": "^3.2.20 || ^2.7.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.24.3",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/vue": "^5.0.4",
    "@types/jest": "^25.2.2",
    "@vue/composition-api": "^1.3.0",
    "jest": "^26.6.3",
    "microbundle": "^0.12.0",
    "release-it": "^13.6.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.7.0-beta",
    "vue-template-compiler": "^2.6.11",
    "vue": "^2.7.13",
    "vue-demi": "0.13.11",
    "vuepress": "^1.5.0",
    "vuepress-plugin-typescript": "^0.2.0"
  },
  "release-it": {
    "github": {
      "tokenRef": "RELEASE_IT_GITHUB_TOKEN_VC",
      "release": true
    }
  },
  "sideEffects": false
}
